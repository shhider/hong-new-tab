{
  "manifest_version": 3,
  "name": "Hong NewTab",
  "version": "0.1",
  "description": "A clear new tab page, listing your specific bookmarks",
  "permissions": [
    "bookmarks",
    "storage",
    "tabs"
  ],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
